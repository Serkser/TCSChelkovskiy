<Window x:Class="TradeCenterAdmin.Views.Windows.SelectNextFloorForWay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TradeCenterAdmin.Views.Windows"
        mc:Ignorable="d"
        Title="Выберите следующий этаж для прокладывания машрута" Height="301.2" Width="451.2">
    <Grid>
        <ListBox ItemsSource="{Binding Floors, Mode=TwoWay}" x:Name="listbox" HorizontalAlignment="Left" Height="173" Margin="30,49,0,0" VerticalAlignment="Top" Width="397"/>
        <TextBlock HorizontalAlignment="Left" Margin="30,28,0,0" TextWrapping="Wrap" Text="Этажи торгового центра" VerticalAlignment="Top"/>
        <Button Click="selectFloor" Content="Выбрать этаж" HorizontalAlignment="Left" Margin="30,227,0,0" VerticalAlignment="Top" Width="397" Height="25"/>

    </Grid>
</Window>
